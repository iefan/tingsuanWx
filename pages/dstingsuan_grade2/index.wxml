<!--index.wxml-->
<view class="container">
  <view class="dsTitle">
    <text style="color: red; font-size: 66rpx;">大 晟 听 算</text>
    <view><text class="descinfo" >（二年级）</text></view>
  </view>
  <view class="userinfo">  
    <button class="buttonNT" bindtap="Grade2Tinsuan"> 100以内加减 </button>
    <button class="buttonNT" bindtap="Grade2_2Tinsuan"> 两位数加减法 </button>
    <button class="buttonNT" bindtap="Grade99Tinsuan"> 表内乘除法 </button>
    <button class="buttonNT" bindtap="Grade99DivTinsuan"> 表内有余数除法 </button>
  </view>
  <view class="usermotto">
    <view>
      <text class="user-motto">{{motto}}</text>
    </view>
    <view>
      <image class="gongzhonghao" src="/Sound/gongzhonghao.jpg"></image>
    </view>
    <view>
    <text style="color: #ff7600; font-size: 30rpx;">▲一位数学老师的笔记▲</text>
    </view>
  </view>
</view>
